    {
      "name": "Debug Direct iOS - Experimental",
      "cwd": "${workspaceFolder}",
      "type": "reactnativedirect",
      "request": "launch",
      "platform": "ios",
      "useHermesEngine": false,
      "target": "device",
      "port": 9221
    },
    {
      "name": "Run Direct iOS - Experimental",
      "cwd": "${workspaceFolder}",
      "type": "reactnativedirect",
      "request": "launch",
      "platform": "ios",
      "enableDebug": false,
      "useHermesEngine": false,
      "target": "device"
    }
  ]
